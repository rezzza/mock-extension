{
    "name": "rezzza/mock-extension",
    "type": "behat-extension",
    "description": "Mock extension for Behat",
    "keywords": ["symfony", "mock", "service"],
    "license": "MIT",
    "homepage": "https://github.com/rezzza/mock-extension",
    "authors": [{
        "name": "Community contributors",
        "homepage": "https://github.com/rezzza/mock-extension/contributors"
    }],

    "require": {
        "php": ">=5.3.2",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "atoum/atoum": "~1.0|~2.0"
    },

    "autoload": {
        "psr-4": { "Rezzza\\MockExtension\\": "src/" }
    },

    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
